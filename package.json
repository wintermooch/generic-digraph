    "karma": "^0.13.15",
    "karma-browserify": "^5.0.0",
    "karma-detect-browsers": "^2.0.2",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-tap": "^1.0.3",
    "standard": "^5.2.2",
    "tape": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wanderer/generic-digraph.git"
